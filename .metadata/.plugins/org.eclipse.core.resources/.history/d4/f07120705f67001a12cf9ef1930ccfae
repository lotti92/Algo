	//back_10430
	//https://www.acmicpc.net/problem/10430
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();
		if(a<1||b<1||c<1||a>10001||b>10001||c>10001) {
			System.exit(0);
		} else {
			System.out.println((a+b)%c);
			System.out.println((a%c + b%c));
			System.out.println((a*b)%c);
			System.out.println((a%c * b%c));
		}
	}
}
